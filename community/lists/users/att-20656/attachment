<font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>MPI_Comm_size(MPI_COMM_WORLD, &amp;numProcs);</span></font></p><p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word">
<font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>MPI_Comm_rank(MPI_COMM_WORLD, &amp;rank);</span></font></p><p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word">
<font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><br></span></font></p><p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word">
<font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>if (rank == 0) {</span></font></p><p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word">
<font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">		</span>srand(time(NULL));</span></font></p><p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word">
<font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">		</span>tmp = (rand() % 100) + 1;<span class="" style="white-space:pre">	</span></span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">		</span>cout &lt;&lt; &quot;generated &quot; &lt;&lt; tmp &lt;&lt; endl;</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>}</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><br></span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>MPI_Barrier(MPI_COMM_WORLD);</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>MPI_Bcast(&amp;tmp, 1, MPI_INT, 0, MPI_COMM_WORLD);</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>MPI_Barrier(MPI_COMM_WORLD);</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><br></span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>if (rank != 0) {</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">		</span>cout &lt;&lt; rank &lt;&lt; &quot; received &quot; &lt;&lt; tmp &lt;&lt; endl;</span></font></p>
<p style="margin:0px 0px 1em;padding:0px;border:0px;vertical-align:baseline;clear:both;word-wrap:break-word"><font color="#000000" face="courier new, monospace"><span style="font-size:14px;line-height:18px"><span class="" style="white-space:pre">	</span>}</span></font></p>
<p></p>

