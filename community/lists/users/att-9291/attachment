<DIV><SPAN lang=EN>
<P>#!/bin/sh</P>
<P>#PBS -N Ad2.0</P>
<P>#PBS -l nodes=3:ppn=8</P>
<P>#PBS -l walltime=100:00:00</P>
<P>date </P>
<P>cd ${PBS_O_WORKDIR}</P>
<P>nprocs=`wc -l &lt; ${PBS_NODEFILE}`</P>
<P>echo "--- PBS_NODEFILE CONTENT ---"</P>
<P>cat $PBS_NODEFILE</P>
<P>echo "--- PBS_NODEFILE CONTENT ---"</P>
<P>echo "Submit host: $(hostname)"</P>
<P>mpiexec -machinefile ${PBS_NODEFILE} -n ${nprocs} ./prewet.out</P>
